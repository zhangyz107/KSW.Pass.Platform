{
    "$schema": "http://json.schemastore.org/template",
    "author": "KSW",
    "classifications": [ "Common", "Console" ],
    "identity": "KSW.ATE01.Template",
    "name": "KSW.ATE01.Template",
    "shortName": "ATE01",
    "sourceName":"KSW.ATE01.Template",
    "tags": {
      "language": "C#",
      "type": "project"
    },
    "symbols": {
        "Namespace":{
            "type": "parameter",
            "datatype":"text",
            "defaultValue": "KSW.ATE01.Template",
            "replaces": "KSW.ATE01.Template"
        },
        "Testplan":{
           "type": "parameter",
            "datatype":"choice",
            "defaultValue": "Excel",
            "choices": [
                {
                    "choice": "Excel",
                    "description": "Excel"
                },
                {
                    "choice": "Csv",
                    "description": "Csv"
                }
            ]
        }
    },
    "Excel": {
        "type": "computed",
        "value": "(TestPlanType == \"Excel\")"
    },
    "Csv": {
        "type": "computed",
        "value": "(TestPlanType == \"Csv\")"
    },
    "sources": [
        {
            "modifiers": [
                {
                    "condition": "Testplan == Excel",
                    "exclude": [
                      "TestPlan/Channel.csv",
                      "TestPlan/Flow.csv",
                      "TestPlan/Level.csv",
                      "TestPlan/Limits.csv",
                      "TestPlan/TestItem.csv",
                      "TestPlan/Timing.csv"
                    ]
                }
            ]
        }
      ]  
  }